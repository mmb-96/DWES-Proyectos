<?xml version="1.0" encoding="UTF-8"?>
<web-app 
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
    version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EmpleadosController</servlet-name>
  	<servlet-class>Controller.EmpleadosController</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>Modic</display-name>
  	<servlet-name>Modic</servlet-name>
  	<servlet-class>Controller.Modic</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	Servlet que manda las modificaciones de un empleado a la DB</description>
  	<display-name>ModificarDB</display-name>
  	<servlet-name>ModificarDB</servlet-name>
  	<servlet-class>Controller.ModificarDB</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmpleadosController</servlet-name>
  	<url-pattern>/Empleados</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Modic</servlet-name>
  	<url-pattern>/Modic</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ModificarDB</servlet-name>
  	<url-pattern>/ModificarDB</url-pattern>
  </servlet-mapping>
</web-app>
